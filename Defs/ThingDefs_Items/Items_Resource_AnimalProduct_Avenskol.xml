    <graphicData>
      <color>(245,245,245)</color>
    </graphicData>
    <statBases>
      <MarketValue>0.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>3.5</hatcherDaystoHatch>
        <hatcherPawn>RA_Dor_AP_Antemilium</hatcherPawn>
